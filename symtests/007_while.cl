class Main {
    main() : SELF_TYPE {
        while 
            let portero : Persona <- getPortero() in
                not plankton.convence(portero)
        loop
            let disfraz : Disfraz <- getNextDisfrazFor(cangrejoCascarudo) 
                in 
                    plankton.vestir(disfraz)
        pool
    };
};